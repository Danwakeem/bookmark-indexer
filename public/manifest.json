{
  "name": "Bookmark Indexer",
  "description": "Better bookmark search",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "bookmarks"
  ],
  "omnibox": { "keyword" : "s" },
  "background": {
    "service_worker": "build/background.js"
  },
  "action": {
    "default_icon": {
      "16": "assets/bookmark-monster-16.png",
      "32": "assets/bookmark-monster-32.png"
      }
  },
  "icons": {
    "16": "assets/bookmark-monster-16.png",
    "32": "assets/bookmark-monster-32.png",
    "48": "assets/bookmark-monster-48.png",
    "128": "assets/bookmark-monster-128.png"
  }
}
